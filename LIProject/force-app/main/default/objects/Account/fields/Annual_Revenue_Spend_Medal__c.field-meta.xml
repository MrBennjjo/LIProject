<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Annual_Revenue_Spend_Medal__c</fullName>
    <description>Indicates whether this customer is a gold/silver/bronze customer regarding spending.</description>
    <externalId>false</externalId>
    <formula>IF(Annual_Revenue_Spend__c &gt; 50000, &apos;GOLD&apos;, IF(Annual_Revenue_Spend__c &gt; 25000, &apos;SILVER&apos;, IF(Annual_Revenue_Spend__c &gt; 10000, &apos;BRONZE&apos;, NULL)))</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Annual Revenue Spend Medal</label>
    <required>false</required>
    <type>Text</type>
    <unique>false</unique>
</CustomField>
